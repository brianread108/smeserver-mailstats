    </thead><tbody>
  <TMPL_ELSE>
		<TMPL_IF NAME="__last__">
			</tbody><tfoot>
			<TMPL_LOOP NAME=cols>
					<td><TMPL_VAR NAME=col></td>
			</TMPL_LOOP>
		<TMPL_ELSE>
			<TMPL_LOOP NAME=cols>
					<TMPL_IF NAME="__last__">
						<td><a href="mailto:<TMPL_VAR NAME=col>"><TMPL_VAR NAME=col></a></td>
					<TMPL_ELSE>
						<td><TMPL_VAR NAME=col></td>
					</TMPL_IF>
			</TMPL_LOOP>			
		</TMPL_IF>
		</tr>
	</TMPL_IF>
</TMPL_LOOP>
</tfoot></table>

<footer><p><TMPL_VAR NAME=BOTTOMBIT></p></footer>
</body></html>
