  <TMPL_IF NAME="__last__">
			</tbody><tfoot>
			<TMPL_LOOP NAME=cols>
					<td><TMPL_VAR NAME=col></td>
			</TMPL_LOOP>
	<TMPL_ELSE>
		<TMPL_LOOP NAME=cols>
			<td><TMPL_VAR NAME=col></td>
